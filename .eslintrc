{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off"
    },
    "plugins": ["ava"],
    "globals": {
        "window": true,
        "d3": false,
        "MathJax": false
    }
}